generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Changelog {
  id          String   @id @default(cuid())
  productName String
  version     String   @default("1.0.0")
  text        String
  createdAt   DateTime @default(now())
}
